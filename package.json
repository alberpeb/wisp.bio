{
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env.development -- next dev",
    "devDB:migrate": "dotenv -e .env.development -- npx prisma migrate dev --name user-entity --create-only && npx prisma generate",
    "devDB:push": "dotenv -e .env.development -- npx prisma db push",
    "dev:build": "dotenv -e .env.development -- prisma generate && next build",
    "build": "dotenv -e .env.production -- prisma generate && next build",
    "start": "dotenv -e .env.production -- next start",
    "lint": "next lint",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc.json",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.14",
    "@hookform/resolvers": "^3.1.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^4.16.1",
    "@types/node": "20.3.1",
    "@types/react": "18.2.13",
    "@vercel/edge-config": "^0.1.0",
    "bcryptjs": "^2.4.3",
    "classnames": "^2.3.2",
    "clsx": "^1.2.1",
    "dotenv-cli": "^7.2.1",
    "next": "^13.4.7",
    "next-auth": "^4.22.1",
    "react": "18.2.0",
    "react-hook-form": "^7.45.0",
    "tailwind-merge": "^1.13.2",
    "tailwindcss": "^3.2.4",
    "typescript": "4.9.4",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.20",
    "@storybook/addon-interactions": "^7.0.20",
    "@storybook/addon-links": "^7.0.20",
    "@storybook/addon-mdx-gfm": "^7.0.20",
    "@storybook/addon-styling": "^1.3.0",
    "@storybook/blocks": "^7.0.20",
    "@storybook/nextjs": "^7.0.20",
    "@storybook/react": "^7.0.20",
    "@storybook/testing-library": "^0.1.0",
    "@types/bcryptjs": "^2.4.2",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.12",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "prisma": "^4.16.1",
    "storybook": "^7.0.20",
    "ts-node": "^10.9.1"
  },
  "prisma": {
    "seed": "ts-node ./prisma/seed.ts"
  }
}
